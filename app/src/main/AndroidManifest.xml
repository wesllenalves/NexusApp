<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nexusapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icons_tennis_64"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InserirAlunoActivity"></activity>
        <activity android:name=".FinanceiroActivity" />
        <activity android:name=".AgendaActivity" />
        <activity android:name=".ValorHoraActivity" />
        <activity android:name=".ListAlunoActivity"
            android:parentActivityName=".AlunoActivity"/>
        <activity android:name=".AlunoAddListAltActivity" />
        <activity android:name=".AlunoActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>